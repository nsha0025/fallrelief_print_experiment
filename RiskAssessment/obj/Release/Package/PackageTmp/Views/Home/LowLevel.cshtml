
@model  RiskAssessment.Models.RiskLevelModel
<!DOCTYPE html>
<html>
<head>
    <link rel="stylesheet" href="styles.css">

    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700,800,900" rel="stylesheet">
    <link href="~/Content/open-iconic-bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/animate.css" rel="stylesheet" />
    <link href="~/Content/owl.carousel.min.css" rel="stylesheet" />
    <link href="~/Content/owl.theme.default.min.css" rel="stylesheet" />

    <link href="~/Content/magnific-popup.css" rel="stylesheet" />
    <link href="~/Content/aos.css" rel="stylesheet" />

    <link href="~/Content/ionicons.min.css" rel="stylesheet" />

    <link href="~/Content/flaticon.css" rel="stylesheet" />

    <link href="~/Content/icomoon.css" rel="stylesheet" />

    <link href="~/Content/style.css" rel="stylesheet" />

    <script src="http://code.jquery.com/jquery-latest.min.js" type="text/javascript"></script>
    <script src="~/Scripts/script.js"></script>

    <link rel="icon" type="image/png" href="~/Content/favicon_192.png" sizes="192x192">
    <link rel="icon" type="image/png" href="~/Content/favicon_160.png" sizes="160x160">
    <link rel="icon" type="image/png" href="~/Content/favicon_96.png" sizes="96x96">
    <link rel="shortcut icon" type="image/x-icon" href="~/Content/favicon.ico" />


    <style>
        .button {
            background-color: #4473c4;
            border: none;
            color: white;
            padding: 10px;
            text-align: center;
            text-decoration: none;
            display: inline-block;
            font-size: 20px;
            margin: 4px 2px;
            cursor: pointer;
            border-radius: 12px;
        }
    </style>
</head>
<body>
    <header>


        <div class="container">
            <div class="ftco-animate">
                <a href="/Home/Index">
                    <img src="~/Content/Fall_Relief_edited_new.png" style="width:20%; height:20%; left:50em" />
                </a>

                <div id='cssmenu'>
                    <ul>
                        <li class="active"><a href="/Home/Index" class="nav-link">Home</a></li>
                        <li><a href="/Home/Assessment" class="nav-link"></a></li>
                        <li><a href="/Home/Tips" class="nav-link"></a></li>
                        <li><a href="/Home/EmergencyHelplines" class="nav-link">Helplines</a></li>
                        <li><a href="/Home/About" class="nav-link">About</a></li>
                    </ul>
                </div>
            </div>
        </div>




    </header>




    <div class="container col-md-12">
        <div class="row" style="background-color:#F5F5F5;padding-left:124px;margin-top:20px">
            <div class="col-md-5" style="margin-top:20px;margin-bottom:20px">
                <div>
                    <h1 style="color:#4473c4;font-size:30px">Assessment Result</h1>
                    <h2>@ViewBag.RiskLevel</h2>
                    <p style="color:black;font-size:18px">

                        Good news, based on your assessment Your answers suggest that you at @ViewBag.RiskLevel level for falls.
                    </p><p style="color:black;font-size:20px">

                        However that does not mean you are immune to falling. You can check out our other assessment or view our general recommendation health tips to prevent fallings using the button on the right.

                    </p>
                </div>
            </div>
            <div class="col-md-5" style="padding-left:30px;padding-right:100px">
                <div class="card" style="  padding:36px;
  margin-top:70px;border:none;
  background-color: #277695;opacity:0.7;margin-bottom:130px">
                    <div class="card-body">
                        <h1 class="card-title" style="font-size:20px;color:white">Home Safety Assessment</h1>
                        <p class="card-text" style="color:white">
                            To find out if your senior’s home is fall proof, by taking the home safety assessment using the button below
                        </p>
                        <a href="#" class="button">Click Here</a>
                    </div>

                </div>
                <!--<a href="/Home/AssessmentResult" class="btn btn-primary" style="padding:4px 30px 4px 30px;margin: 10px 5px">Previous</a>-->

            </div>
        </div>
    </div>



    <footer class="ftco-footer ftco-bg-light" style="border-top:3px; border-top-color:#000000">
        <div class="container">
            <div class="row mb-5">
                <div class="col-md" style="vertical-align:central">
                    <div class="ftco-footer-widget mb-4" style="margin: 50px 0 0 30px">
                        <h1 class="logo">
                            <a href="#" style="letter-spacing:3px">
                                Fall Relief <sup style="text-size-adjust:150%; color:#34b6f8">+</sup>
                            </a>
                        </h1>
                        <div style="color:#34b6f8">
                            <p>
                                <b>Supporting you at every step</b>
                            </p>
                        </div>
                    </div>
                    <div>
                        <h2><b>Crafted with ❤ in Melbourne</b></h2>
                    </div>
                </div>
                <div class="col-md">
                    <div class="ftco-footer-widget mb-4 ml-md-5" style="margin-top:50px">
                        <h2 class="ftco-heading-2"><b>Our Services</b></h2>
                        <ul class="list-unstyled">

                            <li><a href="/Home/Assessment" class="py-1 d-block"><span class="ion-ios-arrow-forward mr-3"></span>Fall Assessment</a></li>
                            <li><a href="/Home/Tips" class="py-1 d-block"><span class="ion-ios-arrow-forward mr-3"></span>Health Tips</a></li>
                            <li><a href="/Home/EmergencyHelplines" class="py-1 d-block"><span class="ion-ios-arrow-forward mr-3"></span>Emergency Helplines</a></li>

                        </ul>
                    </div>
                </div>
                <div class="col-md">
                    <div class="ftco-footer-widget mb-4" style="margin-top:50px">
                        <h2 class="ftco-heading-2"><b>Contact</b></h2>
                        For enquiries and support:
                        <p>
                            Contact us at <a href="mailto:fallrelief@outlook.com" target="_blank"><u>fallrelief@outlook.com</u></a>
                        </p>
                    </div>
                </div>
            </div>

            <hr />

        </div>
    </footer>


    <div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" /><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00" /></svg></div>

    <script src="~/Scripts/jquery.min.js"></script>
    <script src="~/Scripts/jquery-migrate-3.0.1.min.js"></script>
    <script src="~/Scripts/popper.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery.easing.1.3.js"></script>
    <script src="~/Scripts/jquery.waypoints.min.js"></script>
    <script src="~/Scripts/jquery.stellar.min.js"></script>
    <script src="~/Scripts/owl.carousel.min.js"></script>
    <script src="~/Scripts/jquery.magnific-popup.min.js"></script>
    <script src="~/Scripts/aos.js"></script>

    <script src="~/Scripts/jquery.animateNumber.min.js"></script>
    <script src="~/Scripts/scrollax.min.js"></script>
    <script src="~/Scripts/google-map.js"></script>
    <script src="~/Scripts/main.js"></script>

</body>
</html>